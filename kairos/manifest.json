{
  "manifest_version": 2,
  "name": "Website Blocker",
  "version": "1.0",
  "description": "Block websites that you want blocked",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage"
  ],
  "background": {
    "scripts": ["blocker.js"],
    "persistent": false
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
